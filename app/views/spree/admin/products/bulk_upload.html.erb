<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<h3>Bulk upload products</h3>
	</div>
</div>
<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<%= form_tag import_admin_products_path, multipart: true, class: 'form' do %>
			<div class="form-group">
				<%= label_tag 'CSV File' %>
				<%= file_field_tag :file %>	
			</div>
			<div class="form-group">
				<%= submit_tag 'Upload file' %>			
			</div>
		<% end %>
	</div>
</div>