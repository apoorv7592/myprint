			<%= render :partial => 'spree/shared/search' %>
		</li>

        <li class="nav navbar-nav navbar-right">
			<%= link_to "Cart", '/cart/' %>
		</li>
		 <li class="nav navbar-nav navbar-right">
			<%= link_to "Wishlist", '/wishlist/' %>
		</li>
		<li class="nav navbar-nav navbar-right">
			<%= link_to "Log In", '/login/' %>
		</li>

		<li class="nav navbar-nav navbar-right">
		<%if @current_user.present? %>
			<%= link_to "My Account", '/account/'  %>
		<% else %>
			  <%= link_to "Logout", '/logout/' %>
		<% end %>


		</li>

	</ul>
</nav>


	
			
