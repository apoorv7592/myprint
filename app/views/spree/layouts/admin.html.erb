<head data-hook="admin_inside_head">
<%= render partial: 'spree/admin/shared/head' %>
</head>

<body class="admin skin-blue">
<%= render partial: 'spree/admin/shared/header' %>

<p style="margin:50px;"></p>
<div id="wrapper" class="wrapper row-offcanvas row-offcanvas-left">
<div class="navbar navbar-default" >
	<div class="container-fluid">
		<div class="navbar-header">
			<ul class="nav navbar-nav">
				<%= render partial: 'spree/admin/shared/main_menu' %>
			</ul>
		</div>
	</div>
</div>

</section>
</aside>
</div>
<aside class="right-side">
<%= render partial: 'spree/admin/shared/content_header' %>
<section class="content <%= "with-sidebar" if content_for?(:sidebar) %>">
<div class="row">
<div class="<%= main_div_class %> main-content">
<%= flash_alert(flash) %>
<%= render partial: 'spree/admin/shared/alert', collection: session[:alerts] %>
<div class="<%= 'with-sidebar' if content_for?(:sidebar) %>" id="content" data-hook>
<%= render partial: 'spree/admin/shared/table_filter' if content_for?(:table_filter) %>
<%= yield %>
</div>
</div>
<%= render partial: 'spree/admin/shared/sidebar' if content_for?(:sidebar) %>
</div>
</section>
</aside>
<div data-hook="admin_footer_scripts"></div>
</body>
