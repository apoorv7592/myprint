function min(arr)
{
	var min=arr[0],minindex=0;
	for(var i=1 ; i<colnum ; i++)
	{
		if(arr[i]<min)
		{
			min=arr[i];
			minindex=i;
		}
	}
	return minindex;
}	

function getRandomColor() {
    var letters = '0123456789ABCDEF'.split('');
    var color = '#';
    for (var i = 0; i < 6; i++ ) {
        color += letters[Math.floor(Math.random() * 16)];
    }
    return color;
}

function colinitializer(){
		colnum=Math.floor(Math.max($(window).width()/(colwidth+colmargin),3));
		gutter=($(window).width()-colnum*(colwidth))/2;
		for(var i=0 ; i<colnum ; i++)
		colheights[i]=colmargin;
		$("#cardsarea").outerWidth(colnum*(colwidth));
		$("#header").outerWidth(colnum*(colwidth));
		$("#loaderarea").outerWidth(colnum*(colwidth));
		$("#search").outerWidth(Math.min($("#category").offset().left-($("#logo img").width()+$("#logo").offset().left)-30,600));
		$("#category_box").css("right",parseInt($(window).width()-$('#category').offset().left-300)+"px");
		$("#tail").css("right",parseInt($(window).width()-$('#category').offset().left-100)+"px");
		positioncards();
}

function positioncards(){
		$(".card").each(function(){
			var left,top,col;
			col=min(colheights);
			left=col*(colwidth)+gutter;
			top=colheights[col]+$("#cardsarea").offset().top;
			
			$(this).css({"left":left+"px","top":top+"px"});
			
			colheights[col]+=$(this).outerHeight()+colmargin;
		});
		
		var max=0;
		for(var i=0 ; i<colnum ; i++)
			if(colheights[i]>max)
				max=colheights[i];
		
		$("#loaderarea").css({"top":parseInt(max+$("#cardsarea").offset().top)+"px","left":$("#header").offset().left+"px"});
}

