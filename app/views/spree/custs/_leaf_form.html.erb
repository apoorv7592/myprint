<div class="row">
	<div class="col-md-6">
		<%= form_for [s,cust], html: {class: 'form-horizontal'}, remote: true do |f| %>
			<div class="form-group">
				<%= f.label 'Date', class: 'col-sm-3 control-label'%>
				<div class="col-sm-9">
					<%= f.text_field :date_of_event, class: 'form-control' %>	
				</div>
			</div>
			<div class="form-group">
				<%= f.label 'Time', class: 'col-sm-3 control-label' %>
				<div class="col-sm-9">
					<%= f.text_field :date_of_event, class: 'form-control' %>
				</div>
			</div>
			<div class="form-group">
				<%= f.label 'Venue', class: 'col-sm-3 control-label' %>
				<div class="col-sm-9">
					<%= f.text_field :venue, class: 'form-control' %>
				</div>
			</div>
			<div class="form-group">
				<div class="col-md-9 col-md-offset-3">
					<%= f.submit 'Submit', class: 'form-control btn btn-default' %>
				</div>
			</div>
			
		
		<% end %>		
	</div>
	<div class="col-md-6">
		For image of the sutie
	</div>
</div>
